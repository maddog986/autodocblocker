{
    "name": "autodocblocker",
    "displayName": "autodocblocker",
    "description": "Auto generated docblocks using regex. Multiple languages supported.",
    "version": "1.0.2",
    "publisher": "maddog986",
    "author": {
        "name": "Drew Gauderman"
    },
    "icon": "images/autodocblocker.png",
    "license": "GPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/maddog986/autodocblocker.git"
    },
    "bugs": {
        "url": "https://github.com/maddog986/autodocblocker/issues",
        "email": "drew@dpg.host"
    },
    "engines": {
        "vscode": "^1.28.2"
    },
    "categories": [
        "Other",
        "Programming Languages",
        "Formatters"
    ],
    "keywords": [
        "document",
        "doc",
        "autodocblocker",
        "docblock",
        "comment",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "css",
        "sass",
        "al",
        "c",
        "cpp",
        "csharp",
        "dart",
        "flax",
        "fsharp",
        "go",
        "haxe",
        "java",
        "jsonc",
        "kotlin",
        "less",
        "pascal",
        "objectpascal",
        "php",
        "rust",
        "scala",
        "scss",
        "stylus",
        "swift",
        "verilog",
        "vue"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "autodocblocker configuration",
            "properties": {
                "autodocblocker.author": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": null,
                    "description": "Specifies the author string to populate. Overrides the package.json author."
                }
            }
        },
        "commands": [
            {
                "command": "extension.autodocblocker",
                "title": "Run autodocblocker on selected line."
            }
        ],
        "keybindings": [
            {
                "command": "extension.autodocblocker",
                "key": "ctrl+D",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "release": "vsce publish",
        "minor": "vsce publish minor",
        "package": "vsce package"
    },
    "devDependencies": {
        "typescript": "^3.2.2",
        "vscode": "^1.1.26",
        "@types/node": "^10.12.12"
    },
    "dependencies": {
        "dateformat": "^3.0.3",
        "named-js-regexp": "^1.3.3",
        "nunjucks": "^3.1.4"
    }
}