{
    "name": "autodocblocker",
    "displayName": "autodocblocker",
    "description": "Auto generated docblocks using regex. Multiple languages supported.",
    "version": "1.0.0",
    "publisher": "maddog986",
    "author": {
        "name": "Drew Gauderman"
    },
    "license": "GPL-3.0-or-later",
    "engines": {
        "vscode": "^1.28.2"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "document",
        "doc",
        "autodocblocker",
        "comment"
    ],
    "activationEvents": [
        "onCommand:extension.autodocblocker"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.autodocblocker",
                "title": "Run autodocblocker on selected line."
            }
        ],
        "keybindings": [
            {
                "command": "extension.autodocblocker",
                "key": "ctrl+D",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25"
    },
    "dependencies": {
        "dateformat": "^3.0.3",
        "fs": "0.0.1-security",
        "named-js-regexp": "^1.3.3",
        "nunjucks": "latest"
    }
}
